javascript:var a=prompt('Choose background color for your webpage. 1=blue 2=green 3=red 4=orange 5=yellow 6=purple');(function(){if(a==1){document.body.style.background='blue'}else if(a==2){document.body.style.background='green'}else if(a==3){document.body.style.background='red'}else if(a==4){document.body.style.background='orange'}else if(a==5){document.body.style.background='yellow'}else if(a==6){document.body.style.background='purple';}})();
