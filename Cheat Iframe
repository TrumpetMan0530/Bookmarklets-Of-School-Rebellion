javascript:alert("TrumpetMan's Mini Webpage");var URL = prompt('Enter the desired URL\nSuggested:\n\nhttps://www.mathway.com\nhttps://www.desmos.com/calculator');(function () {  var iframe = document.createElement('iframe');  iframe.setAttribute('src', URL);  iframe.setAttribute('id', 'rusic-modal');  iframe.setAttribute('style', 'position: fixed; z-index: 999999; width: 250px; height: 438px; right: 0px; top: 0px; border: 0px solid #8834af; overflow: hidden; background-color: #fff;');  iframe.style.visibility = 'visible';  document.body.appendChild(iframe);  window.addEventListener('keydown', function (event) {    if (event.key === 'e') {      iframe.style.visibility = 'visible';    } else if (event.key === 'd') {      iframe.style.visibility = 'hidden';    }  });})();
