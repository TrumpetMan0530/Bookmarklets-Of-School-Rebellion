javascript:alert("TrumpetMan's Hack Menu\n\n'e' to open, 'd' to hide");var div = document.createElement('div');div.style.width = '200px';div.style.height = '250px';div.style.background = 'black';div.style.color = 'white';div.style.position = 'absolute';div.style.top = '2%';div.style.visibility = 'hidden';div.innerHTML = "TrumpetMan's Hack Menu<br><br>[1. TTC]<br><br>[2. ETS]<br><br>[3. GP]<br><br>[4. CANCEL]";document.body.appendChild(div);window.addEventListener('keydown', function (event) {    if (event.key == 'e') {        div.style.visibility = 'visible';    } else if (event.key == 'd') {        div.style.visibility = 'hidden';    } else if (event.key == '1') {        var a = prompt("TrumpetMan's Tab Cloaker\n\nType a new name for your tab");        document.title = a;    } else if (event.key == '2') {        var URL = prompt("TrumpetMan's emergency tab switcher. Type or paste the emergency URL into the prompt.");        if (URL !== null) {            window.open(URL);        }    } else if (event.key == '3') {        alert("TrumpetMan's Google Proxy");        (function () {            var iframe = document.createElement("iframe");            iframe.setAttribute("src", "https://www.google.com/?igu=1");            iframe.setAttribute("id", "rusic-modal");            iframe.setAttribute("style", "position: fixed; z-index: 999999; width: 1375px; height: 675px; right: 0px; top: 0px; border: 0px solid #8834af; overflow: hidden; background-color:#fff;");            document.body.appendChild(iframe);        })();    } else if (event.key == '4') {        location.reload();    }});
